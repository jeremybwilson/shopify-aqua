{% if settings.facebook_url != blank %}
  <li>
    <a href="{{ settings.facebook_url }}" target="_blank" title="Go to your facebook account">
      {% include 'svg-icon-facebook' %}
    </a>
  </li>
{% endif %}

{% if settings.instagram_id != blank %}
    <li>
      <a href="//instagram.com/{{ settings.instagram_id }}" target="_blank" title="Go to your instagram account">
        {% include 'svg-icon-instagram' %}
      </a>
    </li>
{% endif %}

